.search{
  width:100%;
  height:76rpx;
  display:flex;
  justify-content:center;
  align-items:center;
}
.search>view{
  width:97%;
  height:98%;
  background-color:#f2f2f2;
  border-radius:4px;
  display:flex;
  align-items:center;
}
.search>view>image{
  width:56rpx;
  height:56rpx;
  margin-left:2%;
}
.search>view>input{
  width:90%;
  height:100%;
  font-size:12px;
  padding-left:2%;
}